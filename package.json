{
  "name": "holidays-jp",
  "description": "get Japanese public holidays",
  "version": "1.1.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "bin": {
    "holiday": "./bin/holiday.js"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "csv-parse": "^4.4.5",
    "csv-stringify": "^5.3.3",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "espower-typescript": "^9.0.2",
    "husky": "^3.0.3",
    "iconv-lite": "^0.5.0",
    "lint-staged": "^9.2.1",
    "ltsv": "^1.1.0",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^1.18.2",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "through2": "^3.0.1",
    "typescript": "^3.5.3"
  },
  "files": [
    "bin/holiday.js",
    "data.*",
    "index.js"
  ],
  "license": "MIT",
  "main": "index.js",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/holidays-jp.js.git"
  },
  "scripts": {
    "convert:csv": "node ./npm_script/csv_to_csv.js",
    "convert:js": "node ./npm_script/csv_to_js.js",
    "convert:json": "node ./npm_script/csv_to_json.js",
    "convert:ltsv": "node ./npm_script/csv_to_ltsv.js",
    "convert:ts": "node ./npm_script/csv_to_ts.js",
    "convert:tsv": "node ./npm_script/csv_to_tsv.js",
    "download": "node ./npm_script/download.js",
    "fix": "eslint --cache --fix --ext .js,.ts .",
    "fixpack": "npx fixpack",
    "generate": "run-p generate:*",
    "generate:csv": "yarn run -s convert:csv < ./syukujitsu.csv > ./data.csv",
    "generate:js": "yarn run -s convert:js < ./syukujitsu.csv > ./data.js",
    "generate:json": "yarn run -s convert:json < ./syukujitsu.csv > ./data.json",
    "generate:ltsv": "yarn run -s convert:ltsv < ./syukujitsu.csv > ./data.ltsv",
    "generate:ts": "yarn run -s convert:ts < ./syukujitsu.csv > ./data.ts",
    "generate:tsv": "yarn run -s convert:tsv < ./syukujitsu.csv > ./data.tsv",
    "lint": "eslint --cache --ext .js,.ts .",
    "postgenerate": "rimraf ./syukujitsu.csv",
    "postgenerate:js": "prettier --parser babel --write ./data.js",
    "postgenerate:ts": "prettier --parser typescript --write ./data.ts",
    "pregenerate": "yarn run --silent download > ./syukujitsu.csv",
    "test": "mocha"
  }
}
